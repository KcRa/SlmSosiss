    >
    <rotate
        android:startOffset="0"
        android:fromDegrees="0"
        android:toDegrees="360"
        android:duration="3000"
        android:pivotY="50%"
        android:pivotX="50%"

    />
    <scale
        android:startOffset="0"
        android:fromXScale=".5"
        android:fromYScale=".5"
        android:toXScale="1"
        android:toYScale="1"
        android:pivotX="50%"
        android:pivotY="50%"
        android:duration="3000"

        />
